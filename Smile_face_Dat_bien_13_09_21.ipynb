    "scrolled": true
   },
   "outputs": [],
   "source": [
    "pen.pensize(6)\n",
    "# vẽ mặt cười\n",
    "pen.penup()\n",
    "pen.goto(0,-200)\n",
    "pen.pendown()\n",
    "pen.pencolor(face_color)\n",
    "pen.circle(facesize)"
   ]
