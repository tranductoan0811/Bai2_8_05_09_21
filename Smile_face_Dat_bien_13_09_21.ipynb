   "outputs": [],
   "source": [
    "#đặt biến\n",
    "turtle.bgcolor(\"#C2D4E6\")\n",
    "pen=turtle.Turtle()\n",
    "face_color='#5D7790'\n",
    "facesize=200"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "id": "86d2df72",
